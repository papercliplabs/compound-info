{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
		"plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
		"prettier"
    ],
    "rules": {
		"prettier/prettier": "error",
		"react/prop-types": 0
    },
	"settings": {
		"react": {
			"version": "detetect"
		}
	}
}
